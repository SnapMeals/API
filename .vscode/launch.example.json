{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "program": "uvicorn",
            "console": "integratedTerminal",
            "env": {
                "UVICORN_RELOAD": "true",
                "OPENAI_API_KEY": "your_openai_api_key",
                "OPENAI_ORG_ID": "your_openai_org_id"
            },
            "args": [
                "app.main:app",
                "--reload",
                "--port", "8000"
            ]
        }
        
    ]
}